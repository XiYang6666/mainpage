{
    "files.associations": {
        "*.vue": "vue",
        "*.css": "tailwindcss",
        "*.scss": "tailwindcss"
    },
    "tailwindCSS.includeLanguages": {
        "vue": "html",
        "css": "tailwindcss",
        "scss": "tailwindcss",
        "postcss": "tailwindcss"
    },
    "tailwindCSS.validate": true,
    "css.validate": false,
    "scss.validate": false
}